{
  "name": "@flink/prisma",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "generate:farcaster": "npx prisma generate --schema ./prisma/farcaster.prisma",
    "generate:identity": "npx prisma generate --schema ./prisma/identity.prisma",
    "build": "yarn generate:farcaster && yarn generate:identity && tsc && cp -r generated dist/generated",
    "push:farcaster": "npx prisma db push --schema ./prisma/farcaster.prisma",
    "push:identity": "npx prisma db push --schema ./prisma/identity.prisma"
  },
  "exports": {
    ".": "./dist/index.js",
    "./farcaster": "./dist/farcaster.js",
    "./identity": "./dist/identity.js"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bullmq": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "prisma": "^5.7.1",
    "typescript": "^5.3.3"
  }
}
