{
  "name": "@nook/common",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "build": "yarn db:generate && tsc && cp -r prisma/generated dist/prisma",
    "db:generate": "yarn db:generate:farcaster && yarn db:generate:nook && yarn db:generate:content",
    "db:generate:farcaster": "npx prisma generate --schema ./prisma/schemas/farcaster.prisma",
    "db:generate:nook": "npx prisma generate --schema ./prisma/schemas/nook.prisma",
    "db:generate:content": "npx prisma generate --schema ./prisma/schemas/content.prisma",
    "db:push:farcaster": "npx prisma db push --schema ./prisma/schemas/farcaster.prisma",
    "db:push:nook": "npx prisma db push --schema ./prisma/schemas/nook.prisma",
    "db:push:content": "npx prisma db push --schema ./prisma/schemas/content.prisma"
  },
  "exports": {
    "./queues": "./dist/queues/index.js",
    "./types": "./dist/types/index.js",
    "./clients": "./dist/clients/index.js",
    "./prisma/farcaster": "./dist/prisma/farcaster.js",
    "./prisma/nook": "./dist/prisma/nook.js",
    "./farcaster": "./dist/farcaster/index.js",
    "./content": "./dist/content/index.js",
    "./redis": "./dist/redis/index.js"
  },
  "dependencies": {
    "@farcaster/hub-nodejs": "^0.11.3",
    "@prisma/client": "5.7.1",
    "bullmq": "^5.1.0",
    "metascraper": "^5.45.0",
    "metascraper-audio": "^5.45.0",
    "metascraper-author": "^5.45.0",
    "metascraper-date": "^5.45.0",
    "metascraper-description": "^5.45.0",
    "metascraper-feed": "^5.45.0",
    "metascraper-iframe": "^5.45.0",
    "metascraper-image": "^5.45.0",
    "metascraper-lang": "^5.45.0",
    "metascraper-logo": "^5.45.0",
    "metascraper-logo-favicon": "^5.45.0",
    "metascraper-media-provider": "^5.45.0",
    "metascraper-publisher": "^5.45.0",
    "metascraper-readability": "^5.45.0",
    "metascraper-title": "^5.45.0",
    "metascraper-url": "^5.45.0",
    "metascraper-video": "^5.45.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "prisma": "5.7.1",
    "prisma-json-types-generator": "^3.0.3",
    "typescript": "^5.3.3"
  }
}
