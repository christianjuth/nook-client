{
  "expo": {
    "name": "nook",
    "slug": "nook",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/nook.png",
    "scheme": "nook",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "social.nook"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/nook.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/nook.png"
    },
    "plugins": ["expo-secure-store", "expo-font"],
    "extra": {
      "eas": {
        "projectId": "8d68d9d0-c390-497b-9bf9-294d39f27e4f"
      }
    }
  }
}
